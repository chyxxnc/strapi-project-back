{
  "kind": "collectionType",
  "collectionName": "forms",
  "info": {
    "singularName": "form",
    "pluralName": "forms",
    "displayName": "Form"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
